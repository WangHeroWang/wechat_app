page {
  background-color: #F1F1F1;
}
swiper-item {
  overflow: visible;
  min-height: 30vh;
}
.header {
  background-color: #007AFF;
  width: 92%;
  height: 10vw;
  padding: 0 4%;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
}
.header.status {
    padding-top: 25px;
}
.hover {
  background-color: #eee;
}
.list {
  width: 100%;
  border-bottom: solid 26rpx #f1f1f1;
  font-size: 28rpx;
}
.list .li {
    width: 100%;
    height: 100rpx;
    padding: 0 4%;
    border-bottom: solid 1rpx #e7e7e7;
    display: -webkit-box;
    display: -webkit-flex;
    display: flex;
    -webkit-box-align: center;
    -webkit-align-items: center;
            align-items: center;
}
.list .li.noborder {
      border-bottom: 0;
}
.list .li .icon {
      -webkit-flex-shrink: 0;
              flex-shrink: 0;
      width: 50rpx;
      height: 50rpx;
}
.list .li .icon image {
        width: 50rpx;
        height: 50rpx;
}
.list .li .text {
      padding-left: 20rpx;
      width: 100%;
      color: #666;
}
.list .li .to {
      -webkit-flex-shrink: 0;
              flex-shrink: 0;
      width: 40rpx;
      height: 40rpx;
}
.orderborder {
  border-right: 1px solid #808080;
}
.title {
  background-color: #FFFFFF;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  margin: 0 auto;
  box-sizing: border-box;
  height: 80rpx;
  font-size: 28rpx;
  line-height: 80rpx;
  text-align: center;
}
.title view {
    height: 80rpx;
    line-height: 80rpx;
    padding: 0;
    display: block;
    width: 50%;
    border: none;
}
.title view:after {
      border: none;
      border-radius: 0;
}
.active {
  color: #333;
  font-weight: bold;
  border-radius: 10rpx;
}
.content {
  background: #F8F8F8;
}
swiper {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-flex: 1;
  -webkit-flex: 1 1 auto;
          flex: 1 1 auto;
}
swiper :first-child {
  overflow: visible;
}
.kongindex {
  background-color: #007AFF;
  margin: 0 auto;
}
.kongindex .kongindeximg {
    width: 500rpx;
    text-align: center;
    background-color: #007AFF;
}
.kongindex .kongindeximg image {
      width: 300rpx;
      height: 300rpx;
}

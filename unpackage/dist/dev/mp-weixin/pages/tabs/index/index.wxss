.index-posboxxz {
  box-sizing: border-box;
  padding: 0 20rpx;
  -webkit-animation: fadeInUp .5s .2s ease both;
  -moz-animation: fadeInUp .5s .2s ease both;
}
@-webkit-keyframes fadeInUp {
0% {
    opacity: 0;
    -webkit-transform: translateY(20px);
}
100% {
    opacity: 1;
    -webkit-transform: translateY(0);
}
}
